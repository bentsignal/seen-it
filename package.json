{
  "name": "seen-it",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.chrome.ts",
    "dev:chrome": "vite --config vite.config.chrome.ts",
    "dev:firefox": "vite --config vite.config.firefox.ts",
    "build:chrome": "vite build --config vite.config.chrome.ts",
    "build:firefox": "vite build --config vite.config.firefox.ts",
    "build": "bun run build:chrome && bun run build:firefox",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "preview": "vite preview",
    "icons": "bun scripts/generate-icons.ts"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.33",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/sharp": "^0.32.0",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.1.1",
    "oxlint": "^1.37.0",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "webextension-polyfill": "^0.12.0"
  }
}
